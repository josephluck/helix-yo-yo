{
  "name": "helix-benchmark",
  "version": "1.0.0",
  "description": "Helix benchmark",
  "main": "./src/index.ts",
  "scripts": {
    "dev": "budo ./src/index.ts --live --pushstate --host=localhost --port=8888 --base=/ --css ./node_modules/bootstrap/dist/css/bootstrap.css -- -t babelify -p tsify",
    "dist": "tsc && npm run browserify",
    "browserify": "browserify -g babelify -g uglifyify dist/index.js -o dist/bundle.min.js"
  },
  "author": "Joseph Luck",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babelify": "^7.3.0",
    "@types/node": "^7.0.5",
    "browserify": "^14.1.0",
    "budo": "^9.4.7",
    "tsify": "^3.0.1",
    "tslint": "^4.4.2",
    "tslint-config-standard": "^4.0.0",
    "typescript": "^2.2.1",
    "uglifyify": "^3.0.4",
    "uglifyjs": "^2.4.10"
  },
  "dependencies": {
    "bootstrap": "^3.3.7"
  }
}
